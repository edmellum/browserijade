  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/edmellum/raw/master/LICENSE"
    }
  ],
  "dependencies": {
    "jade": "0.25.x",
    "glob": "3.1.x",
    "underscore": "*"
  },
  "devDependencies": {
